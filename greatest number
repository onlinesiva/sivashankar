#include <stdio.h>

int main()
{
 int a,b,c;
printf("Enter the number a:\t");
scanf("%d",&a);
printf("Enter the number b:\t");
scanf("%d",&b);
printf("Enter the number c:\t");
scanf("%d",&c);
if(a>b)
printf("%d :A is biggest",a);
else if(b>c)
printf("%d :B is biggest",b);
else if(c>a)
printf("%d :C is biggest",c);
getch();
}
